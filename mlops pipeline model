    {
      "cell_type": "code",
      "source": [
        "# Cell 4 ‚Äî write train.py\n",
        "train_code = \"\"\"\n",
        "# src/train.py\n",
        "import joblib\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "import os\n",
        "\n",
        "def train_model(X_train, y_train, save_path='models/model.pkl'):\n",
        "    model = LogisticRegression(max_iter=200, random_state=42)\n",
        "    model.fit(X_train, y_train)\n",
        "    os.makedirs(os.path.dirname(save_path), exist_ok=True)\n",
        "    joblib.dump(model, save_path)\n",
        "    return model\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    print('Run train_model from your pipeline runner')\n",
        "\"\"\"\n",
        "open('/content/ml_pipeline/src/train.py','w').write(train_code)\n",
        "print(\"src/train.py written\")\n"
      ],
