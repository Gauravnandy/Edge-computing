    {
      "cell_type": "code",
      "source": [
        "# Overwrite main.py with correct code\n",
        "main_code = \"\"\"\n",
        "# main.py\n",
        "from src.preprocess import load_and_preprocess_data\n",
        "from src.train import train_model\n",
        "from src.evaluate import evaluate_model\n",
        "import joblib, os\n",
        "\n",
        "def pipeline(save_to_drive=False):\n",
        "    print(\"\\\\nüîÑ Loading & preprocessing data...\")\n",
        "    X_train, X_test, y_train, y_test, scaler = load_and_preprocess_data()\n",
        "\n",
        "    print(\"üèãÔ∏è Training model...\")\n",
        "    model = train_model(X_train, y_train, save_path='models/model.pkl')\n",
        "\n",
        "    print(\"üìä Evaluating...\")\n",
        "    acc, report = evaluate_model(model, X_test, y_test)\n",
        "    print(f\"\\\\n‚úÖ Accuracy: {acc:.4f}\")\n",
        "    print(\"\\\\nClassification Report:\")\n",
        "    print(report)\n",
        "\n",
        "    # Save scaler\n",
        "    os.makedirs('models', exist_ok=True)\n",
        "    joblib.dump(scaler, 'models/scaler.pkl')\n",
        "\n",
        "    if save_to_drive:\n",
        "        drive_path = '/content/drive/MyDrive/ml_pipeline_models'\n",
        "        os.makedirs(drive_path, exist_ok=True)\n",
        "        joblib.dump(model, f'{drive_path}/model.pkl')\n",
        "        joblib.dump(scaler, f'{drive_path}/scaler.pkl')\n",
        "        print(f\"üíæ Model & scaler saved to Google Drive at {drive_path}\")\n",
        "    else:\n",
        "        print(\"\\\\nüíæ Model saved locally at ./models/model.pkl\")\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    pipeline(save_to_drive=False)\n",
        "\"\"\"\n",
        "open('/content/ml_pipeline/main.py','w').write(main_code)\n",
        "print(\"‚úÖ main.py rewritten successfully\")\n"
      ],
      "metadata": {
