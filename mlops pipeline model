    {
      "cell_type": "code",
      "source": [
        "# Cell 5 ‚Äî write evaluate.py\n",
        "eval_code = \"\"\"\n",
        "# src/evaluate.py\n",
        "from sklearn.metrics import accuracy_score, classification_report\n",
        "\n",
        "def evaluate_model(model, X_test, y_test):\n",
        "    preds = model.predict(X_test)\n",
        "    acc = accuracy_score(y_test, preds)\n",
        "    report = classification_report(y_test, preds)\n",
        "    return acc, report\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    print('Run evaluate_model from your pipeline runner')\n",
        "\"\"\"\n",
        "open('/content/ml_pipeline/src/evaluate.py','w').write(eval_code)\n",
        "print(\"src/evaluate.py written\")\n",
        "\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "NTRe0unJHskh",
        "outputId": "5a328ba5-fd09-4a7d-a3fb-31fadc5ea273"
